{
  "name": "workup/nova-tailwind",
  "description": "Tailwind loader for Laravel Nova.",
  "keywords": [
    "laravel",
    "nova",
    "tailwind",
    "css"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.0",
    "workup/nova": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Workup\\Tailwind\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Workup\\Tailwind\\TailwindServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
